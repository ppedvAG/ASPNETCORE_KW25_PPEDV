{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MyKey": "My appsettings.json Value",
  "Position": {
    "Title": "Editor",
    "Name": "Joe Smith"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Debug"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Debug"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 31
        }
      }
    ]
  },
  "ConnectionStrings": {
    "BookstoreDBContext": "Server=(localdb)\\mssqllocaldb;Database=Bookstore001Db;Trusted_Connection=True;MultipleActiveResultSets=true",
    "MovieDbContext": "Server=(localdb)\\mssqllocaldb;Database=RazorPageKurs.Data;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}